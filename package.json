{
	"name": "rmit-ecommerce",
	"version": "1.0.0",
	"description": "RMIT Ecommerce",
	"main": "server/index.js",
	"scripts": {
		"dev": "npm-run-all --parallel dev:*",
		"dev:client": "cd client && npm run dev",
		"dev:server": "cd server && npm run dev",
		"postinstall": "npm-run-all --parallel install:*",
		"install:client": "cd client && npm install",
		"install:server": "cd server && npm install",
		"lint": "eslint . --max-warnings 0",
		"test":"jest"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tomhuynhsg/rmit-ecommerce.git"
	},
	"keywords": [
		"node",
		"express",
		"mongoose",
		"react",
		"redux",
		"redux-thunk",
		"webpack"
	],
	"author": "Tom Huynh (https://github.com/tomhuynhsg)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/tomhuynhsg/rmit-ecommerce/issues"
	},
	"homepage": "https://github.com/tomhuynhsg/rmit-ecommerce#readme",
	"devDependencies": {
		"cross-env": "^5.2.1",
		"eslint": "^9.17.0",
		"eslint-plugin-react": "^7.37.3",
		"eslint-plugin-react-hooks": "^5.1.0",
		"jest": "^29.7.0",
		"npm-run-all": "^4.1.5",
		"supertest": "^7.0.0"
	}
}
